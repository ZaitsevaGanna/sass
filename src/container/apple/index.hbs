<div class="page">
  {{#with header}}
  <div class="header">
    <div class="header_content">
      <div class="header_block">
        <img src="/img/apple.svg" alt="Apple" class="header_icon">
        <nav class="header_block">
          {{#each links}}
          <a href="{{url}}" class="header_link">{{text}}</a>
          {{/each}}
        </nav>
      </div>
      <div class="header_block">
        <img src="/img/search.svg" alt="Search" class="header_icon">
        <img src="/img/profile.svg" alt="Profile" class="header_icon">
      </div>
    </div>
  </div>
  {{#with ad}}
  <div class="header header_white">
    <div class="header_content">
      <div class="header_block">
        <p class="header_description">{{title}}</p>
      </div>
      <div class="header_block">
        {{#with link}}
        <a class=" header_link product_action product_link" href="{{url}}">{{text}}</a>
        {{/with}}
      </div>
      {{/with}}
      {{/with}}
    </div>
  </div>
  {{#with iphone_pro}}
  <section class="product product_big product_iphone_pro product_dark product_top">
    <div class="product_content">
      <h1 class="product_title">{{title}}</h1>
      <p class="product_duscription">{{descriptions}}</p>
      <div class="product_action">
        {{#with link}}
        {{#each text}}
        <a href="{{../link}}" class="product_link">{{this}}</a>
        {{/each}}
        {{/with}}
      </div>
    </div>
  </section>
  {{/with}}

  {{#with iphone}}
  <section class="product product_big product_iphone product_light product_top">
    <div class="product_content">
      <h1 class="product_title">{{title}}</h1>
      <p class="product_duscription">{{descriptions}}</p>
      <div class="product_action">
        {{#with link}}
        {{#each text}}
        <a href="{{../link}}" class="product_link">{{this}}</a>
        {{/each}}
        {{/with}}
      </div>
    </div>
  </section>
  {{/with}}

  {{#with iwatch}}
  <section class="product product_big product_iwatch product_dark product_top">
    <div class="product_content">
      <h1 class="product_title">{{title}}</h1>
      <p class="product_duscription">{{descriptions}}</p>
      <div class="product_action">
        {{#with link}}
        {{#each text}}
        <a href="{{../link}}" class="product_link">{{this}}</a>
        {{/each}}
        {{/with}}
      </div>
    </div>
  </section>
  {{/with}}

  <div class="section_double">
    {{#with trade_in}}
    <section class="product product_small product_trade_in product_light product_top">
      <div class="product_content">
        <h1 class="product_title">{{title}}</h1>
        <p class="product_duscription">{{descriptions}}</p>
        <div class="product_action">
          {{#with link}}

          <a href="{{link}}" class="product_link">{{text}}</a>

          {{/with}}
        </div>
      </div>
    </section>
    {{/with}}
    {{#with conference}}
    <section class="product product_small product_conference product_dark product_bottom">
      <div class="product_content">
        <h1 class="product_title">{{title}}</h1>
        <p class="product_duscription">{{descriptions}}</p>
        <div class="product_action">
          {{#with link}}

          <a href="{{link}}" class="product_link">{{text}}</a>

          {{/with}}
        </div>
      </div>
    </section>
    {{/with}}
  </div>

  <div class="section_double">
    {{#with ipad}}
    <section class="product product_small product_ipad product_light product_top">
      <div class="product_content">
        <h1 class="product_title">{{title}}</h1>
        <p class="product_duscription">{{descriptions}}</p>
        <div class="product_action">
          {{#with link}}

          {{#each text}}
          <a href="{{../link}}" class="product_link">{{this}}</a>
          {{/each}}


          {{/with}}
        </div>
      </div>
    </section>
    {{/with}}
    {{#with small_business}}
    <section class="product product_small product_small_business  product_dark product_top">
      <div class="product_content">
        <h1 class="product_title">{{title}}</h1>
        <p class="product_duscription">{{descriptions}}</p>
