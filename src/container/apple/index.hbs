      </div>
    </section>
    {{/with}}
    {{#with card}}
    <section class="product product_small product_card  product_light product_top">
      <div class="product_content">
        <h1 class="product_title">{{title}}</h1>
        <p class="product_duscription">{{descriptions}}</p>
        <div class="product_action">
          {{#with link}}
          <a href="{{link}}" class="product_link">{{text}}</a>

          {{/with}}
        </div>
      </div>
    </section>
    {{/with}}
  </div>

  <div class="footer">
    <div class="footer_content">
      {{#each footer}}
      <nav class="footer_block">
        <p class="footer_block_title">{{title}}</p>
        {{#each links}}
        <a href="{{url}}" class="footer_link">{{text}}</a>
        {{/each}}
      </nav>
      {{/each}}
    </div>
  </div>
</div>




